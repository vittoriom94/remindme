{
    "name": "RemindMe",
    "version": "0.1",
    "description": "Remind me of a website",
    "permissions": ["activeTab", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "images/remindme_icon.png"
          }
    },
    "icons": {

        "128": "images/remindme_icon.png"
    },
    "manifest_version": 2
  }